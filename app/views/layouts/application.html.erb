<!DOCTYPE html>
<html>
<head>
  <title> MyApp </title>
  
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/> 
  
  <%= stylesheet_link_tag "application", :media => "all" %>

  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag params[:controller] %>
  
  <!--basic styles-->

  <%= stylesheet_link_tag "css/bootstrap.min.css", "css/bootstrap-responsive.min.css" %>
  <!-- <%= stylesheet_link_tag "css/font-awesome.min.css" %> -->

  <link href="//maxcdn.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.min.css" rel="stylesheet">
  
  <!--[if IE 7]>
    <link rel="stylesheet" href="assets/css/font-awesome-ie7.min.css" />
  <![endif]-->

  <!--page specific plugin styles-->

  <%= stylesheet_link_tag params[:controller] %>

  <!--fonts-->

  <!-- <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300" /> -->

  <!--ace styles-->
    
  <%= stylesheet_link_tag  "css/ace.min.css", "css/ace-responsive.min.css", "css/ace-skins.min.css" %>

  <!--[if lte IE 8]>
    <link rel="stylesheet" href="themes/css/ace-ie.min.css" />
  <![endif]-->

  <!--basic scripts-->

  <%= javascript_include_tag "js/jquery-2.0.3.min" %>

  <%= javascript_include_tag "application" %>

  <%= javascript_include_tag "js/bootstrap.min" %>

  <%= javascript_include_tag "js/jquery-ui-1.10.3.custom.min.js" %>
  <%= javascript_include_tag "js/jquery.ui.touch-punch.min.js" %>
  <%= javascript_include_tag "js/jquery.slimscroll.min.js" %>
  <%= javascript_include_tag "js/jquery.easy-pie-chart.min.js" %>
  <%= javascript_include_tag "js/jquery.sparkline.min.js" %>
  <%= javascript_include_tag "js/flot/jquery.flot.min.js" %>
  <%= javascript_include_tag "js/flot/jquery.flot.pie.min.js" %>
  <%= javascript_include_tag "js/flot/jquery.flot.resize.min.js" %>

  <!--ace scripts-->

  <%= javascript_include_tag "js/ace-elements.min.js" %>
  <%= javascript_include_tag "js/ace.min.js" %>

  <!--inline scripts related to this page-->

  <%= javascript_include_tag params[:controller] %> 

</head>
<body class="skin-1">
  <script type="text/javascript">
    dateServerRender = function  () {
      return "<%=date_server%>"  
    }
  </script>
  <!-- Encabezado -->
  <div class="navbar">
    <div class="navbar-inner">
      <div class="container-fluid">
  
        <%= render 'layouts/header' %>

      </div>
    </div>
  </div>

  <!-- Contenedor Principal -->
  <div class="main-container container-fluid" id="main-container">
    
    <a class="menu-toggler" id="menu-toggler" href="#">
      <span class="menu-text"></span>
    </a>

    <!-- SideBar -->
    <div class="sidebar" id="sidebar">
      <!-- ShortCuts Iconos Arriba -->
      <%= render 'layouts/sidebar_shortcuts' %>

      <!-- Lista de Navegacion -->
      <%= render 'layouts/sidebar_navs' %>

      <div class="sidebar-collapse" id="sidebar-collapse">
        <i class="icon-double-angle-left"></i>
      </div>

    </div>

    <!-- Contenedor de Contenido -->
    <div id="main-content" class="main-content">

      <!-- breadcrumbs -->
      <div class="breadcrumbs" id="breadcrumbs">
        
        <%= render 'layouts/breadcrumb' %>
      
      </div>

      <div class="page-content">
      
        <%= yield %>
      
      </div>

    </div>
  
  </div>
  
</body>
</html>